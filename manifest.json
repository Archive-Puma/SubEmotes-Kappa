{
  "name": "SubEmotes? Kappa",
  "version": "1.0.1",
  "description": "Browser extension to visualize private Twitch emoticons",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://twitch.tv/*",
        "*://www.twitch.tv/*"
      ],
      "js": [
        "src/main.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/48.png",
    "48": "icons/48.png",
    "128": "icons/48.png"
  }
}
